{
    "buildCommand": "prisma generate && next build",
    "devCommand": "next dev",
    "installCommand": "npm install",
    "framework": "nextjs",
    "regions": [
        "sin1"
    ],
    "crons": [
        {
            "path": "/api/cron/keep-alive",
            "schedule": "*/5 * * * *"
        },
        {
            "path": "/api/cron/cleanup",
            "schedule": "0 2 * * *"
        }
    ],
    "headers": [
        {
            "source": "/api/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                },
                {
                    "key": "Referrer-Policy",
                    "value": "strict-origin-when-cross-origin"
                }
            ]
        }
    ]
}